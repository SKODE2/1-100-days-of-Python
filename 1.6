# extra

def print_date():
    inputs = input("Enter day, month, year (separated by spaces): ").split()
    date_parts = []
    # If user tab spaces mucht than one in ignores
    for part in inputs:
        if part.strip():
            date_parts.append(part.strip())
    # If user input < 3 columns invalid
    if len(date_parts) == 3:
        day, month, year = date_parts
        print(day, month, year, sep='-')
    else:
        print("Invalid input. Please provide day, month, and year.")

# Call the function to execute the code
print_date()
